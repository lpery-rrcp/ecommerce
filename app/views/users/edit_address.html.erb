<h1>Enter Your Shipping Address</h1>

<%= form_with(model: current_user, url: update_address_users_path, local: true) do |f| %>
  <%= f.label :address %><br>
  <%= f.text_field :address %><br>

  <%= f.label :city %><br>
  <%= f.text_field :city %><br>

  <%= f.label :province_id, "Province" %><br>
  <%= f.collection_select :province_id, Province.all, :id, :name %><br>

  <%= f.submit "Continue to Checkout" %>
<% end %>
